{
  "presets": [
    "es2015",
    "stage-0"
  ],
  "plugins": [
    "es6-promise",
    "transform-decorators-legacy",
    "transform-class-properties",
    "transform-es2015-spread",
    "transform-object-rest-spread",
    [
      "transform-react-jsx",
      {
        "pragma": "h"
      }
    ]
  ],
  "ignore": [
  ]
}
